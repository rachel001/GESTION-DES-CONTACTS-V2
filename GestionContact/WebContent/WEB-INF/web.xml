<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>GestionContact</display-name>
  <servlet>
    <servlet-name>LoginServelt</servlet-name>
    <servlet-class>servelts.LoginServelt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServelt</servlet-name>
    <url-pattern>/LoginServelt</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddContactServelt</servlet-name>
    <servlet-class>servelts.AddContactServelt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddContactServelt</servlet-name>
    <url-pattern>/AddContact</url-pattern>
  </servlet-mapping>
</web-app>