<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>GestionContact</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginServelt</display-name>
    <servlet-name>LoginServelt</servlet-name>
    <servlet-class>servelts.LoginServelt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServelt</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>addContactServelt</display-name>
    <servlet-name>addContactServelt</servlet-name>
    <servlet-class>servelts.addContactServelt</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>addContactServelt</servlet-name>
    <url-pattern>/addContactServelt</url-pattern>
  </servlet-mapping>
</web-app>