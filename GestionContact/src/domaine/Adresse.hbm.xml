<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domaine">
<class name="Adresse" table="Adresse_Table">
  <id name="idAdresse" type="long" column="ID_ADRESSE" >
   	<generator class="increment"/>
  </id>
  <property name="street"><column name="STREET"/></property>
  <property name="city"><column name="CITY"/></property>
  <property name="zip"><column name="ZIP"/></property> 
  <property name="country"><column name="COUNTRY"/></property>
</class>
</hibernate-mapping>